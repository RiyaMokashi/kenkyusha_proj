{
    "data_dir": "./data/csv_clean",
    "results_dir": "./results",
    "test_fraction": 0.2,
    "random_state": 7,
    "noise_kinds": ["label"],
    "noise_levels": [0.1, 0.15, 0.2, 0.25, 0.3, 0.35, 0.4, 0.45, 0.5, 0.55, 0.6,
		     0.65, 0.7, 0.75, 0.8, 0.85, 0.9],
    "disp_accs": 0,
    "disp_elas": 0,
    "disp_rlas": 0,
    "save_ela_fig": 1,
    "save_rla_fig": 1,
    "fig_size": [6, 5],
    "models": [
	{
	    "name": "adaboost6",
	    "module": "sklearn.ensemble",
	    "model": "AdaBoostClassifier",
	    "params": {
		"base_estimator": {
		    "module": "sklearn.tree",
		    "model": "DecisionTreeClassifier",
		    "params": {
			"criterion": "entropy",
			"max_depth": 6,
			"random_state": 7
		    }
		},
		"n_estimators": 100,
		"random_state": 7
	    }
	},
	{
	    "name": "gboost6",
	    "module": "sklearn.ensemble",
	    "model": "GradientBoostingClassifier",
	    "params": {
		"n_estimators": 100,
		"max_depth": 6,
		"learning_rate": 0.1,
		"random_state": 7
	    }
	},
	{
	    "name": "xgboost6",
	    "module": "xgboost",
	    "model": "XGBClassifier",
	    "params": {
		"n_estimators": 100,
		"max_depth": 6,
		"learning_rate": 0.1,
		"booster": "gbtree",
		"reg_alpha": 0.1,
		"reg_lambda": 0.1,
		"n_jobs": 2,
		"random_state": 7
	    }
	}
    ]
}

